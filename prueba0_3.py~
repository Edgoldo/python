#Programa con contenido de operaciones matemáticas
import math

print("Uso del modulo math y funciones matemáticas)
# Salida: 3.141592653589793
print(math.pi)

# Salida: -1.0
print(math.cos(math.pi))

# Salida: 22026.465794806718
print(math.exp(10))

# Salida:  3.0
print(math.log10(1000))

# Salida:  1.1752011936438014
print(math.sinh(1))

# Salida:  720
print(math.factorial(6))
